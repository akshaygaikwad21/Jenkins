pipeline{
    agent any
    stages{
        stage ('pull'){
            steps{
                git branch: 'Jenkins_pipeline', url: 'https://github.com/akshaygaikwad21/Jenkins.git'
            }
        }
        stage ('build'){
            steps{
	    	
                sh ''' 
		cd pom/
		/opt/apache-maven-3.9.9/bin/mvn clean package
		'''
            }
        }
    }
}
