pipeline{
    agent any
    stages{
        stage ('pull'){
            steps{
                git branch: 'Jenkins_pipeline', url: 'https://github.com/akshaygaikwad21/Jenkins.git'
            }
        }
        stage ('build'){
            steps{
                echo 'build created'
            }
        }
        stage ('test'){
            steps{
                echo 'tested'
            }
        }
        stage ('deploy'){
            steps{
                echo 'deployed'
            }
        }
    }
}
   
